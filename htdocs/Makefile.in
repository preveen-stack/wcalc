	all all-am check check-am clean clean-generic clean-libtool \
	clean-local ctags ctags-recursive distclean distclean-generic \
	distclean-libtool distclean-local distclean-tags distdir dvi \
	dvi-am html html-am info info-am install install-am \
	install-data install-data-am install-dvi install-dvi-am \
	install-exec install-exec-am install-html install-html-am \
	install-info install-info-am install-man install-pdf \
	install-pdf-am install-pkgimgdataDATA install-ps install-ps-am \
	install-strip installcheck installcheck-am installdirs \
	installdirs-am maintainer-clean maintainer-clean-generic \
	mostlyclean mostlyclean-generic mostlyclean-libtool pdf pdf-am \
	ps ps-am tags tags-recursive uninstall uninstall-am \
	uninstall-pkgimgdataDATA


# search for the .asciitab files (for the resistivity table, etc)
vpath %.asciitab ../gtk-wcalc:$(top_srcdir)/gtk-wcalc

favicon.ico: $(srcdir)/favicon.xpm
@MISSING_XPMTOPPM_TRUE@	@echo "****************************************************"
@MISSING_XPMTOPPM_TRUE@	@echo "WARNING:  xpmtoppm and/or ppmtowinicon were not found on your"
@MISSING_XPMTOPPM_TRUE@	@echo "          system but $@ is out of date and needs"
@MISSING_XPMTOPPM_TRUE@	@echo "          to be rebuilt.  Changes to $<"
@MISSING_XPMTOPPM_TRUE@	@echo "          will be ignored."
@MISSING_XPMTOPPM_TRUE@	@echo "****************************************************"
@MISSING_XPMTOPPM_FALSE@	${XPMTOPPM} $(srcdir)/favicon.xpm | ${PPMTOWINICON} > $@

clean-local:	
	-rm ${HTML_OUT}

distclean-local:
	-rm AWG.incl

# add the dependency on the .incl files
${HTML_OUT} : ${INCL_FILES}

mex-wcalc : mex-man/whatis.incl
octave-wcalc : octave-man/whatis.incl
sci-wcalc : sci-man/whatis.incl
stdio-wcalc : stdio-man/whatis.incl

.asciitab.incl:
	${FORMAT_TAB} $< > $@

.shtml.html :
@AWK_GENSUB_TRUE@	$(SHTML2HTML) $< | sed 's;/cgi-bin/;${CGIPATH};g' > $@
@AWK_GENSUB_FALSE@	@echo "WARNING:  your awk (${AWK}) does not include the gensub()"
@AWK_GENSUB_FALSE@	@echo "          function.  This prevents the rebuilding of the"
@AWK_GENSUB_FALSE@	@echo "          .html files from the .shtml files.  If you need"
@AWK_GENSUB_FALSE@	@echo "          this functionality, you will need to install gawk."
@AWK_GENSUB_FALSE@	@echo "          By setting the variable AWK in your configure"
@AWK_GENSUB_FALSE@	@echo "          environment, you can force configure to find a"
@AWK_GENSUB_FALSE@	@echo "          particular awk program."
