.txt.m : 
	@echo "****************************************************"
	@echo "Generating ${TARGETPROGRAM} help file: $*.m"
	@echo "****************************************************"
	${AWK} 'BEGIN{s=0;} \
		s==0{ \
			printf("%%%-16s ",toupper($$1)); \
			for(i=2;i<=NF;i++){printf("%s ",$$i);} \
			printf("\n"); \
			s=1; \
			next; \
		} \
		{print "%  " $$0} \
		END{printf("\n\n");}' $< > $@
