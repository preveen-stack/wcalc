@MISSING_NROFF_FALSE@	@echo "****************************************************"
@MISSING_NROFF_FALSE@	cat $< | ${TBL} | ${NEQN} -Tascii | ${GROFF} -man -Tascii | \
@MISSING_NROFF_FALSE@		${AWK} 'BEGIN{s=0;t=0;} \
@MISSING_NROFF_FALSE@		     /^ *$$/ {s=1;next;} \
@MISSING_NROFF_FALSE@		     s==1 { \
