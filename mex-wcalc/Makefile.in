install-pkglibexecDATA: $(pkglibexec_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(pkglibexecdir)" || $(MKDIR_P) "$(DESTDIR)$(pkglibexecdir)"
	@list='$(pkglibexec_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(pkglibexecDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(pkglibexecdir)/$$f'"; \
	  $(pkglibexecDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(pkglibexecdir)/$$f"; \
	done

uninstall-pkglibexecDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(pkglibexec_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(pkglibexecdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(pkglibexecdir)/$$f"; \
	done
